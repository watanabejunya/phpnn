{
    "name": "niisan-tokyo/phpnn",
    "description": "The neural network (NN) written by PHP.",
    "type": "library",
    "license": "MIT",
    "require":{
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "PhpNN\\Foundation\\": "src/",
            "PhpNN\\Models\\": "models/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
